{
	"name": "@dovenv/docs",
	"version": "1.5.9",
	"description": "Documentation utils for dovenv",
	"keywords": [
		"docs",
		"dovenv",
		"markdown",
		"md",
		"pigeonposse",
		"pp",
		"server"
	],
	"homepage": "https://dovenv.pigeonposse.com/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs/guide/plugin/docs",
	"bugs": {
		"url": "https://github.com/pigeonposse/dovenv/issues",
		"email": "dev@pigeonposse.com"
	},
	"repository": {
		"type": "https",
		"url": "https://github.com/pigeonposse/dovenv/",
		"directory": "packages/plugin/docs"
	},
	"funding": {
		"type": "individual",
		"url": "https://pigeonposse.com/?popup=donate"
	},
	"license": "GPL-3.0",
	"author": {
		"name": "Angelo",
		"email": "angelo@pigeonposse.com",
		"url": "https://github.com/angelespejo"
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/main.d.mts",
				"default": "./dist/main.mjs"
			},
			"require": {
				"types": "./dist/main.d.cts",
				"default": "./dist/main.cjs"
			}
		}
	},
	"main": "dist/main.mjs",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"bin": {
		"dovenv-docs": "dist/bin.mjs"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"build-preview": "node dist/bin.mjs --config-path examples/docs.config.js",
		"dev": "tsx examples/ws/main.ts",
		"dev-bin": "tsx src/bin.ts --config-path examples/bin/docs.config.js",
		"dev-og": "tsx src/.vitepress/og/core.example.ts",
		"dev-simple": "tsx examples/simple/main.ts docs",
		"test": "playwright test"
	},
	"dependencies": {
		"@resvg/resvg-js": "2.6.2",
		"@shikijs/vitepress-twoslash": "3.4.2",
		"@vite-pwa/assets-generator": "1.0.0",
		"markdown-it-container": "4.0.0",
		"markdown-it-task-lists": "2.1.1",
		"medium-zoom": "1.1.0",
		"satori": "0.13.1",
		"satori-html": "0.3.2",
		"vite-plugin-full-reload": "1.2.0",
		"vite-plugin-pwa": "1.0.0",
		"vite-plugin-restart": "0.4.2",
		"vitepress": "1.6.3",
		"vitepress-plugin-group-icons": "1.5.5",
		"vitepress-plugin-llmstxt": "0.1.0",
		"vitepress-plugin-rss": "0.3.1",
		"vue": "3.5.14"
	},
	"devDependencies": {
		"@dovenv/core": "workspace:*",
		"@dovenv/repo-config": "workspace:*",
		"@dovenv/utils-media": "workspace:*",
		"@types/markdown-it-container": "2.0.10"
	},
	"peerDependencies": {
		"@dovenv/core": "workspace:*"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
