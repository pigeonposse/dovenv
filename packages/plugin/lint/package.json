{
	"name": "@dovenv/lint",
	"version": "1.5.26",
	"description": "Lint configuration for dovenv",
	"keywords": [
		"eslint",
		"eslint-plugin",
		"commitlint",
		"config",
		"dovenv",
		"lint",
