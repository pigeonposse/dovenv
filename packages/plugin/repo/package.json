{
	"name": "@dovenv/repo",
	"version": "0.0.1",
	"private": true,
	"description": "Media configuration for dovenv",
	"type": "module",
	"exports": {
		".": "./dist/main.mjs"
	},
	"main": "dist/main.js",
	"module": "dist/main.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"build": "unbuild",
		"dev": "tsx examples/main.ts",
		"preview": "node dist/bin.mjs",
		"test": "vitest run -r src --passWithNoTests"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": ".dev/cz-config.cjs"
		}
	},
	"dependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.9",
		"@commitlint/prompt": "^19.5.0",
		"commitizen": "4.3.1",
		"cz-customizable": "7.2.1",
		"husky": "9.1.6",
		"inquirer": "^12.0.1"
	},
	"devDependencies": {
		"dovenv": "workspace:*"
	},
	"peerDependencies": {
		"@dovenv/lint": "workspace:*",
		"@dovenv/utils": "workspace:*"
	}
}
