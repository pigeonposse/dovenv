{
	"name": "@dovenv/repo",
	"version": "1.5.31",
	"description": "Repository tools for any runtime",
	"keywords": [
		"aio",
		"changelog",
		"cli",
		"contributors",
		"dovenv",
		"git",
		"github",
		"library",
		"pigeonposse",
		"pp",
		"publish",
		"repo",
		"update"
	],
	"homepage": "https://dovenv.pigeonposse.com/guide/plugin/repo",
	"bugs": {
		"url": "https://github.com/pigeonposse/dovenv/issues",
		"email": "dev@pigeonposse.com"
	},
	"repository": {
		"type": "https",
		"url": "https://github.com/pigeonposse/dovenv/",
		"directory": "packages/plugin/repo"
	},
	"funding": {
		"type": "individual",
		"url": "https://pigeonposse.com/?popup=donate"
	},
	"license": "GPL-3.0",
	"author": {
		"name": "Angelo",
		"email": "angelo@pigeonposse.com",
		"url": "https://github.com/angelespejo"
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/main.d.mts",
				"default": "./dist/main.mjs"
			},
			"require": {
				"types": "./dist/main.d.cts",
				"default": "./dist/main.cjs"
			}
		}
	},
	"main": "dist/main.mjs",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"dev": "tsx examples/main.ts",
		"preview": "node dist/bin.mjs",
		"test": "vitest run -r src --passWithNoTests"
	},
	"dependencies": {
		"@changesets/cli": "2.29.4",
		"@sizium/core": "0.0.11",
		"husky": "9.1.7"
	},
	"devDependencies": {
		"@dovenv/core": "workspace:*",
		"@dovenv/repo-config": "workspace:*"
	},
	"peerDependencies": {
		"@dovenv/core": "workspace:*"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
