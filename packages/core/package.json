{
	"name": "@dovenv/core",
	"version": "1.5.11",
	"description": "Quickly and easily set up your environment for your code projects",
	"keywords": [
		"aio",
		"bin",
		"cli",
		"development",
		"dovenv",
		"env",
		"library",
		"node",
		"pigeonposse",
		"pp",
		"tasks",
		"toolkit",
		"workspace"
	],
	"homepage": "https://dovenv.pigeonposse.com/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core/guide/core",
	"bugs": {
		"url": "https://github.com/pigeonposse/dovenv/issues",
		"email": "dev@pigeonposse.com"
	},
	"repository": {
		"type": "https",
		"url": "https://github.com/pigeonposse/dovenv/",
		"directory": "packages/core"
	},
	"funding": {
		"type": "individual",
		"url": "https://pigeonposse.com/?popup=donate"
	},
	"license": "GPL-3.0",
	"author": {
		"name": "Angelo",
		"email": "angelo@pigeonposse.com",
		"url": "https://github.com/angelespejo"
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/main.d.mts",
				"default": "./dist/main.mjs"
			},
			"require": {
				"types": "./dist/main.d.cts",
				"default": "./dist/main.cjs"
			}
		},
		"./utils": {
			"import": {
				"types": "./dist/utils.d.mts",
				"default": "./dist/utils.mjs"
			},
			"require": {
				"types": "./dist/utils.d.cts",
				"default": "./dist/utils.cjs"
			}
		}
	},
	"main": "dist/main.mjs",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"bin": {
		"dovenv": "./dist/bin.mjs"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"dev": "tsx examples/main.ts",
		"dev-bin": "tsx src/bin.ts",
		"preview": "node dist/bin.mjs",
		"test": "vitest run -r src --passWithNoTests"
	},
	"dependencies": {
		"@dovenv/utils": "workspace:*",
		"tiny-updater": "3.5.3"
	},
	"devDependencies": {
		"@dovenv/repo-config": "workspace:*"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
